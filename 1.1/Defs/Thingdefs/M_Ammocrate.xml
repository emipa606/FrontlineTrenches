<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>Ammocrate</defName>
    <label>Ammocrate</label>
    <designationCategory>Trenches</designationCategory>
    <researchPrerequisites>
      <li>Trenches2</li>
    </researchPrerequisites>
    <minifiedDef>MinifiedThing</minifiedDef>
    <rotatable>false</rotatable>
    <thingClass>Building_Storage</thingClass>
    <graphicData>
      <texPath>Things/Building/Furniture/ammocrate</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1.25,1.25)</drawSize>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.4</fillPercent>
    <castEdgeShadows>true</castEdgeShadows>
    <stuffCategories>
      <li>Woody</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>4</Mass>
      <WorkToBuild>300</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <description>Ammunition stored here will not deteriorate.</description>
    <size>(1,1)</size>
    <building>
      <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <defaultStorageSettings>
        <priority>Important</priority>
      </defaultStorageSettings>
      <fixedStorageSettings>
        <filter>
          <categories>
            <li>MortarShells</li>
          </categories>
          <thingDefs>
            <li>TR_AmmoHMG</li>
            <li>TR_AmmoLMG</li>
            <li>TR_AmmoMMG</li>
            <li>Shell_HowitzerTR</li>
            <li>Shell_Pak</li>
            <li>Shell_MortarTR</li>
          </thingDefs>
        </filter>
      </fixedStorageSettings>
    </building>
    <inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <surfaceType>Item</surfaceType>
    <canOverlapZones>false</canOverlapZones>
  </ThingDef>
</Defs>
